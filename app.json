{
  "expo": {
    "name": "pctprogresstracker.mobile",
    "slug": "pctprogresstracker.mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/settings.png",
    "scheme": "pctprogresstrackermobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pctprogresstracker.mobile",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["instagram-stories"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/settings.png",
        "backgroundImage": "./assets/images/settings.png",
        "monochromeImage": "./assets/images/settings.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.pctprogresstracker.mobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icontest.png",
          "backgroundColor": "#fdc93c",
          "resizeMode": "contain",
          "dark": {
            "backgroundColor": "#fff"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
